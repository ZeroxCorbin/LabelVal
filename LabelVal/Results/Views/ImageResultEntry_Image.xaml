<UserControl x:Class="LabelVal.Results.Views.ImageResultEntry_Image"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LabelVal.Results.Views"
                xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
                xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
                xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
             xmlns:converters="clr-namespace:LabelVal.Converters"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ImageDetailsTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <converters:ShortenUIDConverter x:Key="shortenUID"/>
            <converters:ObjectIsNull x:Key="objectIsNull"/>
            <converters:DoubleToStringConverter x:Key="doubleToString"/>
            <converters:EqualZeroOrNull x:Key="equalZeroOrNull"/>
        </ResourceDictionary>
    </UserControl.Resources>


</UserControl>
