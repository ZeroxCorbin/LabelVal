                                     md:HintAssist.Hint="" md:HintAssist.IsFloating="True" IsHitTestVisible="False"/>
                            </DockPanel>

                        </DockPanel>
                    </GroupBox>

                    <GroupBox DockPanel.Dock="Top" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="3,0" Background="{DynamicResource MahApps.Brushes.ThemeBackground}">
