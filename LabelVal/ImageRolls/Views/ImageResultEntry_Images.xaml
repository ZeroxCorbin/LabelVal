                                     Width="40" BorderThickness="0" HorizontalContentAlignment="Center"
                                     md:HintAssist.Hint="Inches" md:HintAssist.IsFloating="True" IsHitTestVisible="False" md:HintAssist.FloatingHintHorizontalAlignment="Left"/>
                                <TextBox Margin="0" BorderThickness="0" Text="X" IsHitTestVisible="False"/>
                                <TextBox Text="{Binding V275Image.ImageHeight, Mode=OneTime, StringFormat=N2}"
                                      Width="40" BorderThickness="0" HorizontalContentAlignment="Center"
