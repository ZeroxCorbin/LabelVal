                            </DockPanel>



                        </DockPanel>
                    </GroupBox>

                    <GroupBox DockPanel.Dock="Top" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="3,3" Background="{DynamicResource MahApps.Brushes.ThemeBackground}">
