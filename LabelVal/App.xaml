            <Color x:Key="ColorBlind5a">#FF6600</Color>
            <SolidColorBrush x:Key="ColorBlindBrush5a" Color="{StaticResource ColorBlind5a}"/>
            
            
            <Color x:Key="ColorBlind6">#FFC20A</Color>
            <SolidColorBrush x:Key="ColorBlindBrush6" Color="{StaticResource ColorBlind6}"/>


            <Color x:Key="ColorBlind7">#ede65a</Color>
            <SolidColorBrush x:Key="ColorBlindBrush7" Color="{StaticResource ColorBlind7}"/>


            <Color x:Key="ColorNormal1">Green</Color>
            <SolidColorBrush x:Key="ColorNormalBrush1" Color="{StaticResource ColorNormal1}"/>

            <Color x:Key="ColorNormal2"></Color>
            <SolidColorBrush x:Key="ColorNormalBrush2" Color="{StaticResource ColorNormal2}"/>

            <Color x:Key="ColorNormal3">#589acf</Color>
            <SolidColorBrush x:Key="ColorNormalBrush3" Color="{StaticResource ColorNormal3}"/>

            <Color x:Key="ColorNormal4">#89c3ef</Color>
