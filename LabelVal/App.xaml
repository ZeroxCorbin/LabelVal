            <Color x:Key="SecondaryColorBlindLight">#FFB74D</Color>
            <SolidColorBrush x:Key="SecondaryColorBlindLightBrush" Color="{StaticResource SecondaryColorBlindLight}"/>

            <Color x:Key="PrimaryColorForeground">#212121</Color>
            <SolidColorBrush x:Key="PrimaryColorForegroundBrush" Color="{StaticResource PrimaryColorForeground}"/>
            <Color x:Key="SecondaryColorForeground">#212121</Color>
            <SolidColorBrush x:Key="SecondaryColorForegroundBrush" Color="{StaticResource SecondaryColorForeground}"/>
            <Color x:Key="PrimaryColorForegroundLight">#757575</Color>
            <SolidColorBrush x:Key="PrimaryColorForegroundLightBrush" Color="{StaticResource PrimaryColorForegroundLight}"/>
            <Color x:Key="SecondaryColorForegroundLight">#757575</Color>
            <SolidColorBrush x:Key="SecondaryColorForegroundLightBrush" Color="{StaticResource SecondaryColorForegroundLight}"/>
            <Color x:Key="PrimaryColorForegroundDark">#000000</Color>
            <SolidColorBrush x:Key="PrimaryColorForegroundDarkBrush" Color="{StaticResource PrimaryColorForegroundDark}"/>
            <Color x:Key="SecondaryColorForegroundDark">#000000</Color>
            <SolidColorBrush x:Key="SecondaryColorForegroundDarkBrush" Color="{StaticResource SecondaryColorForegroundDark}"/>
            
            
            <Color x:Key="PrimaryColorBackground">#FFFFFF</Color>
            <SolidColorBrush x:Key="PrimaryColorBackgroundBrush" Color="{StaticResource PrimaryColorBackground}"/>
            <Color x:Key="SecondaryColorBackground">#FFFFFF</Color>
            <SolidColorBrush x:Key="SecondaryColorBackgroundBrush" Color="{StaticResource SecondaryColorBackground}"/>
            <Color x:Key="PrimaryColorBackgroundDark">#F5F5F5</Color>
            <SolidColorBrush x:Key="PrimaryColorBackgroundDarkBrush" Color="{StaticResource PrimaryColorBackgroundDark}"/>
            <Color x:Key="SecondaryColorBackgroundDark">#F5F5F5</Color>
            <SolidColorBrush x:Key="SecondaryColorBackgroundDarkBrush" Color="{StaticResource SecondaryColorBackgroundDark}"/>
            <Color x:Key="PrimaryColorBackgroundLight">#FAFAFA</Color>
            <SolidColorBrush x:Key="PrimaryColorBackgroundLightBrush" Color="{StaticResource PrimaryColorBackgroundLight}"/>
            <Color x:Key="SecondaryColorBackgroundLight">#FAFAFA</Color>
            <SolidColorBrush x:Key="SecondaryColorBackgroundLightBrush" Color="{StaticResource SecondaryColorBackgroundLight}"/>

           

            

        </ResourceDictionary>
    </Application.Resources>
</Application>
