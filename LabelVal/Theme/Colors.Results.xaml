<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Results: Only Monochrome darkened -->

    <!-- Standard -->
    <Color x:Key="Results">#0C89D4</Color>
    <SolidColorBrush x:Key="Results_Brush" Color="{StaticResource Results}" />

    <!-- Red-Green -->
    <Color x:Key="Results_RG">#56B4E9</Color>
    <SolidColorBrush x:Key="Results_RG_Brush" Color="{StaticResource Results_RG}" />

    <!-- Blue-Yellow -->
    <Color x:Key="Results_BY">#0E9FDA</Color>
    <SolidColorBrush x:Key="Results_BY_Brush" Color="{StaticResource Results_BY}" />

    <!-- Monochrome (Darker) -->
    <Color x:Key="Results_M">#8F8F8F</Color>
    <SolidColorBrush x:Key="Results_M_Brush" Color="{StaticResource Results_M}" />
</ResourceDictionary>