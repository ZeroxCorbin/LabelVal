<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Active (runtime-adjustable) brushes. Initially point at standard sets.  -->
    <SolidColorBrush x:Key="ISO_GradeA_Brush_Active" Color="{StaticResource ISO_GradeA}" />
    <SolidColorBrush x:Key="ISO_GradeB_Brush_Active" Color="{StaticResource ISO_GradeB}" />
    <SolidColorBrush x:Key="ISO_GradeC_Brush_Active" Color="{StaticResource ISO_GradeC}" />
    <SolidColorBrush x:Key="ISO_GradeD_Brush_Active" Color="{StaticResource ISO_GradeD}" />
    <SolidColorBrush x:Key="ISO_GradeF_Brush_Active" Color="{StaticResource ISO_GradeF}" />

    <SolidColorBrush x:Key="SectorWarning_Brush_Active" Color="{StaticResource SectorWarning}" />
    <SolidColorBrush x:Key="SectorError_Brush_Active" Color="{StaticResource SectorError}" />

    <SolidColorBrush x:Key="StatusGreen_Brush_Active" Color="{StaticResource StatusGreen}" />
    <SolidColorBrush x:Key="StatusYellow_Brush_Active" Color="{StaticResource StatusYellow}" />
    <SolidColorBrush x:Key="StatusRed_Brush_Active" Color="{StaticResource StatusRed}" />

    <SolidColorBrush x:Key="V275_Brush_Active" Color="{StaticResource V275}" />
    <SolidColorBrush x:Key="V5_Brush_Active" Color="{StaticResource V5}" />
    <SolidColorBrush x:Key="L95_Brush_Active" Color="{StaticResource L95}" />

    <!--  New Active Brushes (point to Standard variants by default)  -->
    <SolidColorBrush x:Key="ImageRoll_Brush_Active" Color="{StaticResource ImageRoll}" />
    <SolidColorBrush x:Key="Results_Brush_Active" Color="{StaticResource Results}" />
    <SolidColorBrush x:Key="LabelBuilder_Brush_Active" Color="{StaticResource LabelBuilder}" />

    <!--  App Brand Active Brushes  -->
    <SolidColorBrush x:Key="AppPrimary_Brush_Active" Color="{StaticResource AppPrimary}" />
    <SolidColorBrush x:Key="AppSecondary_Brush_Active" Color="{StaticResource AppSecondary}" />
    <SolidColorBrush x:Key="AppAccent_Brush_Active" Color="{StaticResource AppAccent}" />
    <SolidColorBrush x:Key="AppInfo_Brush_Active" Color="{StaticResource AppInfo}" />
    <SolidColorBrush x:Key="AppNeutral_Brush_Active" Color="{StaticResource AppNeutral}" />

    <!--  Active Color values  -->
    <Color x:Key="ISO_GradeA_Color_Active" />
    <Color x:Key="ISO_GradeB_Color_Active" />
    <Color x:Key="ISO_GradeC_Color_Active" />
    <Color x:Key="ISO_GradeD_Color_Active" />
    <Color x:Key="ISO_GradeF_Color_Active" />

    <Color x:Key="SectorWarning_Color_Active" />
    <Color x:Key="SectorError_Color_Active" />

    <Color x:Key="StatusGreen_Color_Active" />
    <Color x:Key="StatusYellow_Color_Active" />
    <Color x:Key="StatusRed_Color_Active" />

    <Color x:Key="V275_Color_Active" />
    <Color x:Key="V5_Color_Active" />
    <Color x:Key="L95_Color_Active" />

    <Color x:Key="ImageRoll_Color_Active" />
    <Color x:Key="Results_Color_Active" />
    <Color x:Key="LabelBuilder_Color_Active" />

    <!--  App Brand Active Colors  -->
    <Color x:Key="AppPrimary_Color_Active" />
    <Color x:Key="AppSecondary_Color_Active" />
    <Color x:Key="AppAccent_Color_Active" />
    <Color x:Key="AppInfo_Color_Active" />
    <Color x:Key="AppNeutral_Color_Active" />

    <!--  Active50 (semi-transparent) brushes  -->
    <SolidColorBrush x:Key="ISO_GradeA_Brush_Active50" Color="{StaticResource ISO_GradeA}" />
    <SolidColorBrush x:Key="ISO_GradeB_Brush_Active50" Color="{StaticResource ISO_GradeB}" />
    <SolidColorBrush x:Key="ISO_GradeC_Brush_Active50" Color="{StaticResource ISO_GradeC}" />
    <SolidColorBrush x:Key="ISO_GradeD_Brush_Active50" Color="{StaticResource ISO_GradeD}" />
    <SolidColorBrush x:Key="ISO_GradeF_Brush_Active50" Color="{StaticResource ISO_GradeF}" />

    <SolidColorBrush x:Key="SectorWarning_Brush_Active50" Color="{StaticResource SectorWarning}" />
    <SolidColorBrush x:Key="SectorError_Brush_Active50" Color="{StaticResource SectorError}" />

    <SolidColorBrush x:Key="StatusGreen_Brush_Active50" Color="{StaticResource StatusGreen}" />
    <SolidColorBrush x:Key="StatusYellow_Brush_Active50" Color="{StaticResource StatusYellow}" />
    <SolidColorBrush x:Key="StatusRed_Brush_Active50" Color="{StaticResource StatusRed}" />

    <SolidColorBrush x:Key="V275_Brush_Active50" Color="{StaticResource V275}" />
    <SolidColorBrush x:Key="V5_Brush_Active50" Color="{StaticResource V5}" />
    <SolidColorBrush x:Key="L95_Brush_Active50" Color="{StaticResource L95}" />

    <SolidColorBrush x:Key="ImageRoll_Brush_Active50" Color="{StaticResource ImageRoll}" />
    <SolidColorBrush x:Key="Results_Brush_Active50" Color="{StaticResource Results}" />
    <SolidColorBrush x:Key="LabelBuilder_Brush_Active50" Color="{StaticResource LabelBuilder}" />

    <!--  App Brand Active50 Brushes  -->
    <SolidColorBrush x:Key="AppPrimary_Brush_Active50" Color="{StaticResource AppPrimary}" />
    <SolidColorBrush x:Key="AppSecondary_Brush_Active50" Color="{StaticResource AppSecondary}" />
    <SolidColorBrush x:Key="AppAccent_Brush_Active50" Color="{StaticResource AppAccent}" />
    <SolidColorBrush x:Key="AppInfo_Brush_Active50" Color="{StaticResource AppInfo}" />
    <SolidColorBrush x:Key="AppNeutral_Brush_Active50" Color="{StaticResource AppNeutral}" />

    <!--  Active50 Color values  -->
    <Color x:Key="ISO_GradeA_Color_Active50" />
    <Color x:Key="ISO_GradeB_Color_Active50" />
    <Color x:Key="ISO_GradeC_Color_Active50" />
    <Color x:Key="ISO_GradeD_Color_Active50" />
    <Color x:Key="ISO_GradeF_Color_Active50" />

    <Color x:Key="SectorWarning_Color_Active50" />
    <Color x:Key="SectorError_Color_Active50" />

    <Color x:Key="StatusGreen_Color_Active50" />
    <Color x:Key="StatusYellow_Color_Active50" />
    <Color x:Key="StatusRed_Color_Active50" />

    <Color x:Key="V275_Color_Active50" />
    <Color x:Key="V5_Color_Active50" />
    <Color x:Key="L95_Color_Active50" />

    <Color x:Key="ImageRoll_Color_Active50" />
    <Color x:Key="Results_Color_Active50" />
    <Color x:Key="LabelBuilder_Color_Active50" />

    <!--  App Brand Active50 Colors  -->
    <Color x:Key="AppPrimary_Color_Active50" />
    <Color x:Key="AppSecondary_Color_Active50" />
    <Color x:Key="AppAccent_Color_Active50" />
    <Color x:Key="AppInfo_Color_Active50" />
    <Color x:Key="AppNeutral_Color_Active50" />

</ResourceDictionary>