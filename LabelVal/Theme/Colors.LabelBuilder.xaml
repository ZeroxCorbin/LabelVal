<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- LabelBuilder: Only Monochrome darkened -->

    <!-- Standard -->
    <Color x:Key="LabelBuilder">#6B2FC9</Color>
    <SolidColorBrush x:Key="LabelBuilder_Brush" Color="{StaticResource LabelBuilder}" />

    <!-- Red-Green -->
    <Color x:Key="LabelBuilder_RG">#CC79A7</Color>
    <SolidColorBrush x:Key="LabelBuilder_RG_Brush" Color="{StaticResource LabelBuilder_RG}" />

    <!-- Blue-Yellow -->
    <Color x:Key="LabelBuilder_BY">#7F41D8</Color>
    <SolidColorBrush x:Key="LabelBuilder_BY_Brush" Color="{StaticResource LabelBuilder_BY}" />

    <!-- Monochrome (Darker) -->
    <Color x:Key="LabelBuilder_M">#636363</Color>
    <SolidColorBrush x:Key="LabelBuilder_M_Brush" Color="{StaticResource LabelBuilder_M}" />
</ResourceDictionary>