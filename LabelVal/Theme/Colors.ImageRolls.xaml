<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--
        ImageRolls Color Group (Teal family)
        Positioned between cyan (Results) and green (StatusGreen) while remaining distinct.
    -->

    <!-- Standard -->
    <Color x:Key="ImageRoll">#008E9B</Color>
    <SolidColorBrush x:Key="ImageRoll_Brush" Color="{StaticResource ImageRoll}" />

    <!-- Red-Green -->
    <Color x:Key="ImageRoll_RG">#009E73</Color>
    <SolidColorBrush x:Key="ImageRoll_RG_Brush" Color="{StaticResource ImageRoll_RG}" />

    <!-- Blue-Yellow -->
    <Color x:Key="ImageRoll_BY">#0094A7</Color>
    <SolidColorBrush x:Key="ImageRoll_BY_Brush" Color="{StaticResource ImageRoll_BY}" />

    <!-- Monochrome -->
    <Color x:Key="ImageRoll_M">#7FBBC1</Color>
    <SolidColorBrush x:Key="ImageRoll_M_Brush" Color="{StaticResource ImageRoll_M}" />
</ResourceDictionary>